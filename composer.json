{
    "type": "project",
    "license": "proprietary",
    "name": "jperat/resume",
    "description": "Simple basice resume template",
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.11",
        "easycorp/easyadmin-bundle": "^4.0.0",
        "friendsofsymfony/jsrouting-bundle": "^3.0",
        "imagine/imagine": "^1.2",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.2",
        "sensio/framework-extra-bundle": "^6.0",
        "symfony/asset": "^5.2.3",
        "symfony/console": "^5.2.3",
        "symfony/dotenv": "^5.2.3",
        "symfony/expression-language": "^5.2.3",
        "symfony/flex": "^1.1",
        "symfony/form": "^5.2.3",
        "symfony/framework-bundle": "^5.2.3",
        "symfony/mime": "^5.2.3",
        "symfony/monolog-bundle": "^3.1",
        "symfony/process": "^5.2.3",
        "symfony/property-access": "^5.2.3",
        "symfony/property-info": "^5.2.3",
        "symfony/proxy-manager-bridge": "^5.2.3",
        "symfony/security-bundle": "^5.2.3",
        "symfony/serializer": "^5.2.3",
        "symfony/templating": "^5.2.3",
        "symfony/translation": "^5.2.3",
        "symfony/twig-bundle": "^5.2.3",
        "symfony/validator": "^5.2.3",
        "symfony/web-link": "^5.2.3",
        "symfony/webpack-encore-bundle": "^1.0",
        "symfony/yaml": "^5.2.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.80",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^5.2.3",
        "symfony/css-selector": "^5.2.3",
        "symfony/debug-bundle": "^5.2.3",
        "symfony/maker-bundle": "^1.11",
        "symfony/phpunit-bridge": "^5.3",
        "symfony/stopwatch": "^5.2.3",
        "symfony/web-profiler-bundle": "^5.2.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5.2.3"
        }
    }
}
